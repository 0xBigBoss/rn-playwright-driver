{
	"name": "@0xbigboss/rn-driver-r3f",
	"version": "0.1.0",
	"description": "R3F (React Three Fiber) integration for rn-playwright-driver",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./helpers": {
			"types": "./dist/helpers.d.ts",
			"import": "./dist/helpers.mjs",
			"require": "./dist/helpers.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsgo --noEmit",
		"check": "bun run typecheck && bun run lint"
	},
	"keywords": [
		"react-native",
		"react-three-fiber",
		"r3f",
		"three.js",
		"playwright",
		"e2e",
		"testing"
	],
	"author": "0xbigboss",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/0xbigboss/rn-playwright-driver",
		"directory": "packages/r3f"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"peerDependencies": {
		"@react-three/fiber": ">=8.0.0",
		"react": ">=18.0.0",
		"three": ">=0.150.0"
	},
	"dependencies": {
		"@0xbigboss/rn-playwright-driver": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@react-three/fiber": "^9.1.2",
		"@types/node": "^25.0.3",
		"@types/react": "^19.1.8",
		"@types/three": "^0.178.0",
		"@typescript/native-preview": "^7.0.0-dev.20251227.1",
		"react": "^19.1.0",
		"three": "^0.175.0",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	}
}
